! ime
URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot
! color
URxvt*.depth: 32
URxvt.background: [100]#000000
URxvt.foreground: [100]#d8d8d8
! font
URxvt.font: xft:monospace:antialias=true:autohinting=true:size=9
URxvt.letterSpace: -2
! scroll bar
URxvt.scrollBar: false
! pointer
URxvt.cursorBlink: true

! 0: black
*color0:  #242424
*color8:  #242424
! 1: red
*color1:  #9f6767
*color9:  #9f6767
! 2: green
*color2:  #92ac68
*color10: #92ac68
! 3: yellow
*color3:  #d0d293
*color11: #d0d293
! 4: blue
*color4:  #9aacc3
*color12: #9aacc3
! 5: magenta
*color5:  #bb77a4
*color13: #bb77a4
! 6: cyan
*color6:  #77bbb5
*color14: #77bbb5
! 7: white
*color7:  #777777
*color15: #777777
