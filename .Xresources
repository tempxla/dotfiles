! ime
URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot
! color
!URxvt*.depth: 32
URxvt.background: #1a1a1a
URxvt.foreground: #e4e4e4
! font
URxvt.font: xft:MigMix 2M:antialias=true:autohinting=true:pixelsize=12,style=regular
URxvt.letterSpace: 0
! scroll bar
URxvt.scrollBar: false
! pointer
URxvt.cursorBlink: true
! Disable the Ctrl+Shift binding
URxvt.iso14755: false
URxvt.iso14755_52: false
! Disable Alt+S Keybind
URxvt.perl-ext-common: "default,-searchable-scrollback"

! 0: black
*color0:  #242424
*color8:  #242424
! 1: red
*color1:  #9f6767
*color9:  #9f6767
! 2: green
*color2:  #92ac68
*color10: #92ac68
! 3: yellow
*color3:  #d0d293
*color11: #d0d293
! 4: blue
*color4:  #9aacc3
*color12: #9aacc3
! 5: magenta
*color5:  #bb77a4
*color13: #bb77a4
! 6: cyan
*color6:  #77bbb5
*color14: #77bbb5
! 7: white
*color7:  #777777
*color15: #777777

!XScreenSaver
xscreensaver.newLoginCommand:
